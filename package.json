{
  "name": "nk",
  "version": "1.0.0",
  "description": "NodeJS",
  "author": "zxg",
  "main": "start.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development babel-node start.js",
    "pro": "cross-env NODE_ENV=production babel-node start.js",
    "build": "babel ./NK --out-dir NKDist",
    "start": "node ./NKDist/start.js"
  },
  "keywords": [
    "node",
    "node.js",
    "koa.js"
  ],
  "babel": {
    "compact": false,
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@log4js-node/smtp": "^1.1.0",
    "antd": "^4.0.4",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "echarts": "^4.7.0",
    "element-ui": "^2.13.0",
    "get-port": "^5.1.1",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^4.2.0",
    "http-proxy-middleware": "^1.0.3",
    "jsencrypt": "^3.0.0-rc.1",
    "jsonwebtoken": "^5.7.0",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-favicon": "^2.1.0",
    "koa-helmet": "^4.0.0",
    "koa-mount-routes": "^1.0.1",
    "koa-router": "^7.4.0",
    "koa-server-http-proxy": "^0.1.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "log4js": "^6.1.2",
    "merge": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.16.0",
    "mysql": "^2.18.1",
    "path-to-regexp": "^6.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sdp-transform": "^2.14.0",
    "shortid": "^2.2.15",
    "swiper": "^5.3.7",
    "typeorm": "^0.2.24",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.0",
    "vue-loader": "^15.9.1",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.1.6",
    "webpack": "^4.42.1",
    "webpack-merge": "^4.2.2"
